<body>
    <table class="table table-bordered table-striped">
    <thead>
    <tr>
    <th>Time Submitted:</th>
    <th>Entry:</th>
    </tr>
    </thead>
    <tbody>
  <% for(i=0; i<entries.length; i++) { %>
    <tr>
    <td class="times"> <%= times[i] %> </td>
    <td class="entries"> <%= entries[i] %> </td>
    </tr>
  <% } %>
  </tbody>
  </table>
  <div id="journalBackWrapper">
    <a href="/" id="journalBack" class="btn btn-success">Go Back</a>
  </div>  
